{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsc --project tsconfig.server.json && cross-env NODE_ENV=development EXTERNAL_SERVER_URL='http://localhost:3001' INTERNAL_SERVER_URL='http://localhost:3001' node server/index.mjs",
    "build": "tsc --project tsconfig.server.json && vite build && vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "node server/index.mjs",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint --fix --ext .ts,.tsx",
    "prettier": "prettier --write .",
    "test": "jest ./",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@reduxjs/toolkit": "^2.5.1",
    "axios": "^1.8.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.0.2",
    "express": "^4.21.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.3",
    "styled-components": "^6.1.16",
    "serialize-javascript": "*",
    "vite": "^3.0.7",
    "vite-plugin-commonjs": "^0.10.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@testing-library/react": "^15.0.7",
    "@types/cookie-parser": "^1",
    "@types/jest": "^28.1.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "husky": "^9.1.7",
    "jest": "^28",
    "jest-environment-jsdom": "^29.0.1",
    "jiti": "^2.4.2",
    "lefthook": "^1.3.9",
    "prettier": "^2.7.1",
